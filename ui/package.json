{
  "name": "digital-sourcing",
  "description": "Digital Sourcing",
  "version": "1.0.0",
  "private": true,
  "author": "Sam",
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "build": "gatsby build",
    "clean": "rimraf public",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "dev": "gatsby develop",
    "format": "prettier --write \"src/**/*.md\"",
    "start": "npm run dev",
    "test": "npm run type-check && npm run lint",
    "type-check": "tsc",
    "lint": "tslint --config ./tslint.json --project ./",
    "lint:fix": "tslint --project ./ --fix",
    "check": "npm run type-check && npm run format && npm run lint:fix"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@gov.au/animate": "^1.0.13",
    "@gov.au/body": "^2.2.0",
    "@gov.au/buttons": "^3.0.7",
    "@gov.au/core": "^3.4.2",
    "@gov.au/footer": "^3.0.5",
    "@gov.au/grid-12": "^2.1.0",
    "@gov.au/header": "^4.1.12",
    "@gov.au/headings": "^2.0.11",
    "@gov.au/inpage-nav": "^3.0.6",
    "@gov.au/link-list": "^3.0.8",
    "@gov.au/main-nav": "^1.0.8",
    "@gov.au/page-alerts": "^2.1.3",
    "@gov.au/searchbox": "^0.2.2",
    "@gov.au/select": "^2.0.11",
    "@gov.au/side-nav": "^5.0.7",
    "@gov.au/table": "^0.2.0",
    "@gov.au/text-inputs": "^2.1.2",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "gatsby": "^2.18.1",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-canonical-urls": "^2.1.15",
    "gatsby-plugin-catch-links": "^2.1.25",
    "gatsby-plugin-google-analytics": "^2.1.35",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sass": "^2.1.23",
    "gatsby-plugin-sharp": "^2.3.2",
    "gatsby-plugin-typescript": "^2.1.19",
    "gatsby-remark-copy-linked-files": "^2.1.30",
    "gatsby-remark-images": "^3.1.32",
    "gatsby-remark-prismjs": "^3.3.24",
    "gatsby-remark-responsive-iframe": "^2.2.27",
    "gatsby-remark-smartypants": "^2.1.16",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-json": "^2.2.18",
    "gatsby-transformer-remark": "^2.6.37",
    "moment": "^2.24.0",
    "node-sass": "^4.13.0",
    "polished": "^3.4.1",
    "prism-themes": "^1.3.0",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "typescript": "^3.5.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/node": "^12.12.11",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.8.4",
    "@types/react-helmet": "^5.0.14",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "eslint": "^6.6.0",
    "eslint-config-blvd": "^1.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^2.0.1",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "tslint": "^5.20.1"
  },
  "pancake": {
    "auto-save": true,
    "plugins": true,
    "ignore": [],
    "json": {
      "enable": false,
      "location": "pancake/",
      "name": "pancake",
      "content": {
        "name": true,
        "version": true,
        "dependencies": true,
        "path": true,
        "settings": true
      }
    },
    "js": {
      "minified": true,
      "modules": false,
      "location": "pancake/js/",
      "name": "pancake.min.js"
    },
    "css": {
      "minified": false,
      "modules": false,
      "browsers": [
        "last 2 versions",
        "ie 8",
        "ie 9",
        "ie 10"
      ],
      "location": "pancake/css/",
      "name": "pancake.min.css"
    },
    "sass": {
      "modules": false,
      "location": "pancake/sass/",
      "name": "pancake.scss"
    },
    "react": {
      "location": "pancake/react/"
    }
  }
}
